{
  "name": "greenspoon-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Backend API for Green Spoon cloud kitchen app",
  "main": "server.js",
  "scripts": {
    "start": "node --env-file=.env server.js",
    "dev": "node --env-file=.env server.js",
    "test": "node --test test/*.test.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "node --env-file=.env prisma/seed.js",
    "rotate:seed-passwords": "node --env-file=.env scripts/rotate-seeded-passwords.js"
  },
  "prisma": {
    "seed": "node --env-file=.env prisma/seed.js"
  },
  "dependencies": {
    "@sentry/node": "^9.24.0",
    "@prisma/client": "^6.6.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "pino": "^9.9.4",
    "pino-http": "^10.5.0",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "prisma": "^6.6.0"
  }
}
